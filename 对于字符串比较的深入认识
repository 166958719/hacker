#define _CRT_SECURE_NO_WARNINGS 1
#include <stdio.h>
#include <string.h>
int main()
{
	if ('a' == 'a')
		printf("you are ritght");
	else
		printf("you are wrong");
	if ('az' > 'ab')
		printf("\nyou are ritght");
	else
		printf("\nyou are wrong");//由此可知，字符的比较取决于ASCII，字符串的比较则是依次对比直到遇到不同并比较对应的ASCII来
	                              //决定字符串的比较结果


	return 0;
}
